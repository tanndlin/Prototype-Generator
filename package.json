{
  "name": "prototype-generator",
  "displayName": "Prototype Generator",
  "description": "Creates prototpyes of functions in your C code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:prototype-generator.createPrototypes",
    "onCommand:prototype-generator.createMethod",
    "onCommand:prototype-generator.lint"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "prototype-generator.createPrototypes",
        "title": "Create Prototypes",
        "category": "Prototype Generator"
      },
      {
        "command": "prototype-generator.createMethod",
        "title": "Create Method",
        "category": "Prototype Generator"
      },
      {
        "command": "prototype-generator.lint",
        "title": "Lint Code",
        "category": "Prototype Generator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
